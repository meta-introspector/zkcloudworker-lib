{
  "name": "zkcloudworker",
  "description": "zkCloudWorker library",
  "keywords": [
    "cloud",
    "proofs",
    "proving",
    "minanft",
    "socialcap",
    "library",
    "TypeScript",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
    "web3",
    "zero knowledge"
  ],
  "version": "0.2.9",
  "homepage": "https://docs.zkcloudworker.com",
  "repository": "git@zkcloudworker.github.com:zkcloudworker/zkcloudworker-lib.git",
  "main": "src/index.ts",
  "author": "dfst.io",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "compile": "rm -rf dist/lib && tsc && tsc --build tsconfig.web.json",
    "test": "jest",
    "coverage": "jest --coverage",
    "login": "yarn npm login",
    "publish": "yarn compile && cd dist && yarn npm publish --access public && cd .."
  },
  "dependencies": {
    "axios": "^1.6.8",
    "o1js": "^0.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "slowTestThreshold": 1500,
    "testTimeout": 10800000,
    "verbose": true,
    "roots": [
      "tests"
    ],
    "setupFilesAfterEnv": [
      "./jest-config.ts"
    ]
  },
  "packageManager": "yarn@4.1.1"
}
